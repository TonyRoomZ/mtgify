  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mpaulweeks/mtgify/issues"
  },
  "homepage": "https://github.com/mpaulweeks/mtgify#readme",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "devDependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.5.0",
    "jest": "^23.6.0",
    "tinyify": "^2.5.2",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "aws-sdk": "^2.610.0",
    "event-stream": "^4.0.1",
    "node-fetch": "^2.6.0",
