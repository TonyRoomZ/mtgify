{
  "name": "mtgify",
  "version": "1.0.0",
  "description": "assorted tools for MTG apps",
  "main": "index.js",
  "scripts": {
    "ci": "jest --watch",
    "test": "jest",
    "build": "rm dist/*.js && browserify lib/index.js -t babelify -p tinyify > dist/autocard.js",
